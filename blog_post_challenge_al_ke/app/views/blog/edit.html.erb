<h3>Update Blog Post</h3>

<%= form_with model: @blog, local:true, method: 
:patch do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    <br />
    <br />
    <%= form.label :post %>
    <%= form.text_field :post %>
    <br />
    <br />
    <%= form.submit "Update Blog Post" %>
    <% end %>
