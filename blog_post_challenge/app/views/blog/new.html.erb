<h2>Add a blog category</h2>

<%= form_with model: @category, local: true do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    < br/>
    <%= form.label :content %>
    <%= form.text_field :content %>
    < br/>
    <%= form.submit 'Please add blog category' %>
  <% end %>
