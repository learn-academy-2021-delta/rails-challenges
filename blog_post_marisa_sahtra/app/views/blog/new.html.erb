<h1>Wanna add a new blog post? Do so here! </h1>
<%= form_with model: @blog, local: true do |form| %>
  <%= form.label :title %>
  <br />
  <%= form.text_field :title %>
  <br />
  <%= form.label :content %>
    <br />
  <%= form.text_field :content %>
    <br />
  <%= form.submit "Add Blog" %>
<% end %>

<p> <%= link_to "home", blogs_path%> </p>